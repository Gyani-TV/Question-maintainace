
<div class="container bg-faded py-3">
    <h1 class="text-center">Centering in Bootstrap 4</h1>
    <div class="row">
        <div class="col text-center">use <code>.text-center</code> for display:inline elements</div>
    </div>
    <hr>
    <div class="row">
        <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Panel heading</div>
  <div class="panel-body">
    <p>...</p>
  </div>


    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Level Name</th>
      <th scope="col">Type</th>
      <th scope="col">Handle</th>
    </tr>
  </thead>
  <tbody>
    {{#each data  }}
    {{log caption}}
    <tr>
      <th scope="row">{{@key}}</th>
      <td>{{name}}</td>
      <td>{{type}}</td>
      <td><a type="button" href="/levels/turn/{{_id}}">Choose this level</a></td>
    </tr>
    {{/each }}

  </tbody>
</table>
</div>
    </div>
    <hr>
<nav class="navbar navbar-default ">
  <div class="container">
      <p class="navbar-text active">Signed in as {{ ui.pageparams.empId }}</p>
    <p class="navbar-text "><a type="button" href="/levels/add" class="btn btn-default navbar-btn">Add a level</a></p>
  </div>
</nav>
    <hr>
</div>